<div class="container">
  <div class="column_flex">
    <div class="card_services">
      <h1>Service <%= @service.name.capitalize %></h1>
      <img src="https://www.fillmurray.com/60/60" alt="steve">
    </div>

    <div id="accordion">
    <%# div card Numéro Important  %>
        <div class="card">
          <button class="card_service_phone collapsed" data-toggle="collapse" data-target="#collapseOne" aria-expanded="false" aria-controls="collapseOne">
          <h1>Numéros Important</h1>
          </button>
          <div id="collapseOne" class="collapse" aria-labelledby="headingOne" data-parent="#accordion">
            <div class="card-body">
             <%= @service.phone_number %>
            </div>
          </div>
        </div>

    <%# div card Journée type  %>
        <div class="card">
          <button class="card_service_day collapsed" data-toggle="collapse" data-target="#collapseTwo" aria-expanded="false" aria-controls="collapseTwo">
          <h1>Journée Type</h1>
          </button>
          <div id="collapseTwo" class="collapse" aria-labelledby="headingTwo" data-parent="#accordion">
            <div class="card-body">
              <%= @service.typical_workday %>
            </div>
          </div>
        </div>

    <%# div card Pathologies prévalentes  %>
        <div class="card">
          <button class="card_service_pathology collapsed" data-toggle="collapse" data-target="#collapseThree" aria-expanded="false" aria-controls="collapseThree">
          <h1>Pathologies prevalentes</h1>
          </button>
          <div id="collapseThree" class="collapse" aria-labelledby="headingThree" data-parent="#accordion">
            <div class="card-body">
              <%= @service.predominant_disease %>
            </div>
          </div>
        </div>

    <%# div card Medicaments prévalents  %>
        <div class="card">
          <button class="card_service_pathology collapsed" data-toggle="collapse" data-target="#collapseFour" aria-expanded="false" aria-controls="collapseFour">
          <h1>Medicaments prevalents</h1>
          </button>
          <div id="collapseFour" class="collapse" aria-labelledby="headingFour" data-parent="#accordion">
            <div class="card-body">
            <%= @service.predominant_drugs %>
            </div>
          </div>
        </div>
    </div>


        <div class="blog-card">
        <div class="description">
        <h1>Gestion Toxiques</h1>
        </div>
          </div>


  </div>
</div>
